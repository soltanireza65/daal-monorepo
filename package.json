{
  "name": "daal-monorepo",
  "private": true,
  "scripts": {
    "dev:user": "npx lerna run start:dev --scope=user",
    "dev:wallet": "npx lerna run start:dev --scope=wallet",
    "dev": "concurrently \"npm run dev:user\" \"npm run dev:wallet\" -n"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2",
    "lerna": "^8.1.2"
  },
  "volta": {
    "node": "20.12.2"
  }
}
